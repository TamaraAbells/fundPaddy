    <section id="subscribe" >
        <div class="container">
            <div class="row">
                <div class="col-md-12 wow fadeInDown" data-wow-delay=".8s">
                    <div class="block">
                        <div class="title text-center">
                            <h2>Share your views & stay Connected</h2>
                            <p>Send a message, suggestion or idea to the fundPaddy general messageBoard :)</p>
                        </div>
                        
                        
<!-- 
                        <form class="form-inline text-center col-sm-12 col-xs-12" role="form"> -->
                        <%= form_for @message, :url=> {controller: :static_pages, action: :send_message}, :html=> {class: "form-inline text-center col-sm-12 col-xs-12"}  do |f| %>

                            <div class="row">
                            <div class="col-sm-4 col-lg-4">
                            </div>
                            <div class="col-sm-4 col-lg-4">
                            <div class="form-group">

                                <%= f.text_field :email, placeholder: 'Your email',  id: 'signup-form', required: true %>
                               <!--  <input type="text" placeholder="your email" class="form-control" id="signup-form" >
                                 -->
                                
                            </div> <hr/>
                             <div class="form-group">

                                <%= f.text_area :content, placeholder: "Enter your thoughts here", class: 'form-control', id: 'thoughts', required: true %>
                          <!--       <textarea type="text" name="thoughts" placeholder="Enter your thougts here :)" class="form-control" id="thoughts"> </textarea>
                                 -->
                                
                            </div> <hr/>

                           
                            <%= f.submit "Send", class: 'btn btn-default btn-block btn-signup' %>
                       <!--      <a href="" type="submit" class="btn btn-default btn-block btn-signup">
                                 <i class="fa fa-paper-plane"></i>
                            
                            </a> -->
                          
                            </div>
                        <% end %>
                    <!--     </form> -->
                    </div>
                    

                </div>
            </div>
        </div>
    </section>

